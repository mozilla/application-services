namespace suggest {

};

[Error]
interface SuggestError {
    Other(string reason);
};

dictionary IngestLimits {
    u64? records;
};

dictionary Suggestion {
    string block_id;
    string advertiser;
    string iab_category;
    string full_keyword;
    string title;
    string url;
    sequence<u8>? icon;
    string? impression_url;
    string? click_url;
};

interface SuggestionProvider {
    [Throws=SuggestError]
    constructor([ByRef] string path);

    [Throws=SuggestError]
    sequence<Suggestion> query([ByRef] string keyword);

    void interrupt();

    [Throws=SuggestError]
    void ingest([ByRef] IngestLimits limits);
};
